<% if(model) { %>
<header class="header">
  <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
    <% var title = model.get('title') === undefined ? 'ais' : model.get('title'); %>
    <a href="/" class="main-title navbar-brand"><%= title %></a>
    <ul class="nav navbar-nav navbar-right">
      <% if (model.get('user')) { %>
        <li><p class="navbar-text navbar-right">Logged in as <strong>Elizar Pepino</strong></p></li>
        <li><a href="/account-settings" class="account-settings"><span class="glyphicon glyphicon-cog"></span> Account Settings</a></li>
        <li><a href="#logout" class="logout"><span class="glyphicon glyphicon-log-out"></span> Log Out</a></li>
      <% } else { %>
          <li><p class="navbar-text navbar-right">You are currently not <strong>Logged In </strong></p></li>
      <% } %>
    </ul>
  </nav>
</header>

<div class="main">
  <div class="container">
    
  </div>
</div>

<div class="footer">
  <div class="container">
    <p class="muted credit pull-left">Copyright 2013 <a href="<%= model.get('company').url %>"><%= model.get('company').name %></a> and <a href="<%= model.get('team').url %>"><%= model.get('team').name %></a>.</p>
    <p class="muted credit pull-right"><a href="/help">Help</a> / <a href="/faq">FAQ</a> / <a href="/tos">Contact Us</a></p>
  </div>
</div>
<% } %>
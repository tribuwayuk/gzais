<td><%= model.get('assets').length %></td>
<td><%= model.get('first_name') + ' ' + model.get('last_name') %></td>
<% var dob = new Date(model.get('date_of_birth')) %>
<td><%= dob.getMonth() + 1 + '/' + dob.getDate() + '/' + dob.getFullYear() %></td>
<td><%= model.get('email') %></td>
<td><%= model.get('user_role') %></td>
<% var de = new Date(model.get('date_employed')) %>
<td><%= de.getMonth() + 1 + '/' + de.getDate() + '/' + de.getFullYear() %></td>
<td>
  <button type="button" data-toggle="tooltip" data-placement="bottom" title="" class="btn btn-default btn-xs tool-tip view-details" data-original-title="View Details">
    <span class="glyphicon glyphicon-search"></span>
  </button>
  <button type="button" data-toggle="modal" data-target="#edit-modal" data-placement="bottom" title="" class="btn btn-default btn-xs tool-tip edit-employee" data-original-title="Edit">
    <span class="glyphicon glyphicon-edit"></span>
  </button>
  <button type="button" data-toggle="modal" data-target="#reset-modal" data-placement="bottom" title="" class="btn btn-default btn-xs tool-tip reset-password" data-original-title="Reset Password">
    <span class="glyphicon glyphicon-lock"></span>
  </button>
  <button type="button" data-toggle="tooltip" data-placement="bottom" title="" class="btn btn-danger btn-xs tool-tip delete-employee" data-original-title="Delete">&nbsp;x&nbsp;</button>
</td>
<script>
  $('.tool-tip').tooltip();
</script>
<div class="modal-dialog">
        <div class="modal-content">
          <!-- FORM -->
          <form role="form" id="editform">
            <div class="modal-header">
              <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
              <h4 class="modal-title" id="myModalLabel">Edit Asset</h4>
            </div>
            <div class="modal-body">

              <div class="form-group row">
                <label for="asset_name">Name</label>
                <input type="text" class="form-control" id="asset_name" value="<%=model.get('asset_name')%>">
              </div>
              <div class="form-group">
                <label for="asset_description">Description</label>
                <input type="text" class="form-control" id="asset_name" value="<%=model.get('asset_description')%>">
              </div>
              <div class="form-group row">
                <div class="col-xs-6">
                  <label for="asset_type">Type</label>
                  <input type="text" class="form-control" id="asset_type" value="<%=model.get('asset_type')%>">
                </div>
                <div class="col-xs-6">
                  <label for="date_purchased">Date Purchased</label>
                  <input type="text" id="date_purchased" class="form-control date-purchased" id="date_purchased" value="<%=model.get('date_purchased')%>">
                  <script>
                    var nowTemp = new Date();
                    var now = new Date(nowTemp.getFullYear(), nowTemp.getMonth(), nowTemp.getDate(), 0, 0, 0, 0);
   
                    $('.date-purchased').datepicker({
                      onRender: function(date) {
                        return date.valueOf() > now.valueOf() ? 'disabled' : '';
                      }
                    });
                  </script>
                </div>
              </div>
              <div class="form-group row">
                <div class="col-xs-6">
                  <label for="status">Status</label>
                  <select id="status" class="status-picker" id="status">
                    <option value="working">Working</option>
                    <option value="defective">Defective</option>
                  </select>
                  <script>
                  $('.status-picker').selectpicker();
                  </script>
                </div>
                <div class="col-xs-6">
                  <label for="serial_number">Serial Number</label>
                  <input type="text" class="form-control" id="serial_number" value="<%=model.get('serial_number')%>">
                </div>
              </div>
              <div class="form-group">
                <label for="supplier">Supplier</label>
                <input type="text" class="form-control" id="supplier" value="<%=model.get('supplier')%>">
              </div>
              <div class="form-group">
                <label for="reason">Reason for Purchase?</label>
                <input type="text" class="form-control" id="reason" value="<%=model.get('reason')%>">
              </div>

            </div>
            <div class="modal-footer">
              <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
              <button type="submit" class="btn btn-edit">
                <span class="glyphicon glyphicon-ok-sign"></span> Save Changes</button>
            </div>
          </form>
          <!-- FORM END -->
        </div>
        <!-- /.modal-content -->
      </div>